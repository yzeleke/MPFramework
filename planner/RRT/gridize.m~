function [output, output2, source, goal] = gridize(obstacle,resolution_x, resolution_y, lower_bound_x, lower_bound_y, upper_bound_x, upper_bound_y, source_input, goal_input)
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here

source(1) = [source_input -
source = ceil((source - [lower_bound_x,lower_bound_y])/resolution_x) + 1; 
goal = ceil((goal - [lower_bound_x,lower_bound_y])/resolution_x) + 1; 


upper_bound_i = ceil((upper_bound_x - lower_bound_x)/resolution_x) + 1;
upper_bound_j = ceil((upper_bound_y - lower_bound_y)/resolution_y) + 1;
output = ones(upper_bound_j, upper_bound_i);


for i = 1:upper_bound_i
    for j = 1:upper_bound_j
        x = (i - 1)*resolution_x + lower_bound_x;
        y = (j - 1)*resolution_y + lower_bound_y;
        if (abs(x - obstacle.X) < (0.5*obstacle.Length+obstacle.safeDistanceX))& (abs(y - obstacle.Y) < (0.5*obstacle.Length+obstacle.safeDistanceY))
            output(j,i) = 0;
        end
    end
end
end

